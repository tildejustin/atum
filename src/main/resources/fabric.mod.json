  "authors": [
    {
      "name": "Void_X_Walker",
      "contact": {
        "homepage": "https://github.com/VoidXWalker/"
      }
    }
  ],
  "contributors": [
    "contaria",
    "tildejustin"
  ],
  "license": "MIT",
  "icon": "assets/atum/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.voidxwalker.autoreset.Atum"
    ]
